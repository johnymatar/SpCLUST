<tool id="gclust-fednafull" name="GClust-EDNAFULL Fast" version="1.1.0">
	<description>Takes a FASTA file as input and gives back the gnomes grouping using fast alignment and EDNAFULL</description>
	<command><![CDATA[
		/home/johny/GClust/gclust-FE.sh $input $output
	]]></command>
	<inputs>
		<param name="input" type="data" format="fasta"
		label="Input a fasta file" help="fasta file contains sequences"/>
	</inputs>
	<outputs>
		<data name="output" type="data" format="txt" label="Clustering-FE"/>
	</outputs>
</tool>
