<tool id="spclust-p250" name="SpCLUST-PAM250" version="1.1.0">
	<description>Takes a FASTA file as input and gives back the gnomes grouping using PAM250</description>
	<command><![CDATA[
		/home/johny/SpCLUST/spclust-P250.sh $input $output
	]]></command>
	<inputs>
		<param name="input" type="data" format="fasta"
		label="Input a fasta file" help="fasta file contains sequences"/>
	</inputs>
	<outputs>
		<data name="output" type="data" format="txt" label="Clustering-P250"/>
	</outputs>
</tool>
